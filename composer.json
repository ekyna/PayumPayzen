{
    "name": "ekyna/payum-payzen",
    "description": "Payum PayZen (Systempay, Scellius) gateway",
    "type": "component",
    "authors": [
        {
            "name": "Etienne Dauvergne",
            "homepage": "https://ekyna.com"
        }
    ],
    "keywords": [
        "php",
        "payum",
        "payzen",
        "systempay",
        "scellius"
    ],
    "homepage": "https://github.com/ekyna/PayumPayzen",
    "license": [
