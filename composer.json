        "symfony/process": ">3.4,<7.0",
        "psr/log": "^1.0|^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0|^8.0",
        "php-http/guzzle6-adapter": "^2.0"
    },
    "autoload": {
        "psr-4": {
